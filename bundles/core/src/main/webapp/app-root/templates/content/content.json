{
  "jcr:primaryType": "cq:Template",
  "jcr:title": "wcm.io Handler Sample Content",

  "allowedPaths": "^/content/wcm-io-samples/.+$",
  "allowedChildren": "^/apps/wcm-io-samples/core/.*$",

  "jcr:content": {
    "jcr:primaryType": "cq:PageContent",
    "sling:resourceType": "wcm-io-samples/core/components/content/page/content",


    "text": {
      "jcr:primaryType": "nt:unstructured",
      "sling:resourceType": "core/wcm/components/text/v2/text",
      "textIsRich": true

    },

    "image": {
      "jcr:primaryType": "nt:unstructured",
      "sling:resourceType": "core/wcm/components/image/v2/image"

    }
  }
}
